<%- include("header") -%>
        <div class="box" id="heading">
            <h1><%= listTitle %> </h1>
        </div>

        <div class="box">
            <% for(var i=0;i<newItem.length;i++) { %>
                 <div class="item">
                    <input type="checkbox">
                    <p><%= newItem[i] %></p>
                 </div>
            <% } %>
            <form class="item" action="/" method="post">
                <input type="text" placeholder="enter task" name="task" autocomplete="off">
                <button type="submit" name="button" value=<%= listTitle %> >+</button>
            </form>

        </div>
<%- include("footer") -%>